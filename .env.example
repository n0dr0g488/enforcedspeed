# Copy this file to .env and fill in real values.
# DO NOT commit .env (it contains secrets).

DATABASE_URL=postgresql+pg8000://enforcedspeed:enforcedspeed_password@localhost:5433/enforcedspeed_local
REDIS_URL=redis://localhost:6379/0

# Cloudflare R2 (S3-compatible)
R2_ENDPOINT=https://<account_id>.r2.cloudflarestorage.com
R2_ACCESS_KEY_ID=YOUR_R2_ACCESS_KEY_ID
R2_SECRET_ACCESS_KEY=YOUR_R2_SECRET_ACCESS_KEY
R2_QUARANTINE_BUCKET=enforcedspeed-ticket-quarantine
R2_PREFIX=tickets/

# Google Vision (optional for OCR)
# Preferred: point to a downloaded service-account JSON file (Windows example below).
# Option A (recommended): point Google SDK at your service account JSON file
# GOOGLE_APPLICATION_CREDENTIALS=C:\ES_SECRETS\google_vision_sa.json
# Alternate: put the entire service account JSON in this env var (single-line JSON).
# Option B: set GOOGLE_CREDENTIALS_JSON to either a JSON FILE PATH or a JSON STRING
# GOOGLE_CREDENTIALS_JSON=C:\ES_SECRETS\google_vision_sa.json
GOOGLE_CREDENTIALS_JSON=
